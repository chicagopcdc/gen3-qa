{
  "query": "query ($filter: JSON) {
    _aggregation {
      case (accessibility: accessible, filter: $filter) { 
        _totalCount 
      } 
    } 
  }",
  "variables": {
    "filter": {
      "AND": [
        {
          "<": {
            "age_at_index": 30
          } 
        }, 
        {
          "=": {
            "stroke": "Yes"
          }
        }
      ]
    }
  }
}